{
  "author": "Roman Barron",
  "manifest_version": 3,
  "name": "Deepfake Analysis Extension",
  "version": "1.0.0",
  "description": "Analyze Facial Images Online",
  "permissions": [
    "storage",
    "activeTab",
    "tabs",
    "notifications"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["https://*/*"],
      "js": ["contentScript.js"]
    }
  ],
  "action": {
    "default_icon": {
      //"16": "images/icon16.png",
      //"24": "images/icon24.png",
      //"32": "images/icon32.png"
    },
    "default_popup": "popup.html"
  }
}
